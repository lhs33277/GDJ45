<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE configuration 
PUBLIC "-//mybatis.org//DTD Config 3.0//EN" 
"http://mybatis.org/dtd/mybatis-3-config.dtd" >

<configuration>

	<!-- mapUnderscoreToCamelCase : NOTICE_NO == noticeNo로 인식하는 옵션 -->
	<settings>
		<setting name="mapUnderscoreToCamelCase" value="true"/>
	</settings>
	
	<!-- typeAlias : com.goodee.ex09.domain.NoticeDTO == NoticeDTO로 인식하는 옵션 -->
	<typeAliases>
		<typeAlias type="com.goodee.ex09.domain.NoticeDTO" alias="NoticeDTO" />
	</typeAliases>
	
	<!-- 마이바티스 홈페이지에 있는 옵션들. -->
	
</configuration>